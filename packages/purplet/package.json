{
  "name": "purplet",
  "version": "2.0.0-next.18",
  "description": "A simple framework to build modern Discord bots.",
  "keywords": [
    "discord",
    "bot",
    "api",
    "framework",
    "typescript"
  ],
  "homepage": "https://purplet.js.org",
  "bugs": "https://github.com/CRBT-Team/Purplet/issues",
  "repository": "github:CRBT-Team/Purplet",
  "license": "Apache-2.0",
  "contributors": [
    "Dave Caruso <me@paperdave.net> (https://github.com/paperdave)",
    "Clembs <clembs@clembs.com> (https://github.com/Clembs)"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./internal": "./dist/internal.js",
    "./types": "./dist/types.js",
    "./env": "./dist/env.js"
  },
  "main": "./dist/index.js",
  "types": "index.d.ts",
  "bin": "./dist/cli.js",
  "files": [
    "dist",
    "CHANGELOG.MD",
    "LICENSE",
    "README.md",
    "*.d.ts"
  ],
  "scripts": {
    "build": "shared-config-build",
    "changeset": "cd ../../ && pnpm changeset",
    "dev": "rollup -c --watch",
    "prepack": "pnpm run build"
  },
  "dependencies": {
    "@paperdave/logger": "^2.0.6",
    "@paperdave/utils": "1.4.1",
    "@purplet/builders": "workspace:*",
    "@purplet/gateway": "workspace:*",
    "@purplet/polyfill": "workspace:1.1.0",
    "@purplet/rest": "workspace:*",
    "@purplet/serialize": "workspace:*",
    "@purplet/utils": "workspace:1.0.0",
    "@rollup/plugin-alias": "^4.0.0",
    "@rollup/plugin-commonjs": "^23.0.1",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "chalk": "^5.1.2",
    "chokidar": "^3.5.3",
    "dedent": "^0.7.0",
    "discord-api-types": "^0.37.14",
    "esbuild": "^0.15.12",
    "fast-equals": "^4.0.3",
    "rollup": "^3.2.3",
    "rollup-plugin-all-external": "^1.0.0",
    "rollup-plugin-esbuild": "^4.10.1",
    "tweetnacl": "^1.0.3",
    "vite": "3.0.7",
    "wrap-ansi": "^8.0.1",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@purplet/shared-config": "workspace:*",
    "@types/dedent": "^0.7.0",
    "@types/prompts": "^2.0.14",
    "@types/yargs": "^17.0.11"
  },
  "peerDependencies": {
    "bufferutil": "^4.0.6",
    "erlpack": "^0.1.4",
    "utf-8-validate": "^5.0.9",
    "zlib-sync": "^0.1.7"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.6",
    "erlpack": "^0.1.4",
    "utf-8-validate": "^5.0.9",
    "zlib-sync": "^0.1.7"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
